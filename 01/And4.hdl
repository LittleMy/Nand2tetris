/**
 * 4-bit-wise And gate: for i = 0..15: out[i] = a[i] And b[i]
 */

CHIP And4 {
    IN a[4], b[4];
    OUT out[4];

    PARTS:
    
    Nand(a=a[0], b=b[0], out=w0);
    Nand(a=a[1], b=b[1], out=w1);
    Nand(a=a[2], b=b[2], out=w2);
    Nand(a=a[3], b=b[3], out=w3);
    Not4(in[0]=w0, in[1]=w1, in[2]=w2, in[3]=w3, out=out[0..3]);
   // Not(in=w0, out=out[0]);
   // Not(in=w1, out=out[1]);
   // Not(in=w2, out=out[2]);
   // Not(in=w3, out=out[3]);
    // Put your code here.
}
